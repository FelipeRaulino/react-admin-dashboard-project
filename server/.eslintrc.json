{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "warn",
            "double"
        ],
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
        ]
    }
}